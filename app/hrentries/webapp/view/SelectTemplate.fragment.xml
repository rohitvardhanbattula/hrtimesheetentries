<core:FragmentDefinition xmlns:odata="sap.ui.comp.odata"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:t="sap.ui.table"
    xmlns:core="sap.ui.core">
    <Dialog id="id_dialog_selecttempname" title="{i18n>ListofTemplates}" class="sapUiSmallMargin">
        <content>
            <VBox id="id_select_vbox">
                <!-- rows="{/TempTable}" -->
                <t:Table id="idTable" width="700px" rows="{/TempTable}" rowSelectionChange="onRowSelect" selectionMode="Single">
                    <t:columns>
                        <t:Column id="id_column_Temp_id" label="Template ID">
                            <t:template>
                                <Text id="tab_text_id_Temp_Id2" text="{TempID}" />
                            </t:template>
                        </t:Column>
                        <t:Column id="id_column_Temp_desc" label="Template Description">
                            <t:template>
                                <Text id="tab_text_id_Temp_Desc2" text="{TempDesc}" />
                            </t:template>
                        </t:Column>
                    </t:columns>
                </t:Table>
            </VBox>
        </content>
        <footer>
            <Toolbar id="id_select_temp_name_toolbar">
                <content>
                    <ToolbarSpacer id="id_select_temp_name_spacer" />
                    <Button id="id_select_temp_name_entry_close" text="{i18n>closebutton}" press="oncloseTempNameSelect" />
                </content>
            </Toolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
